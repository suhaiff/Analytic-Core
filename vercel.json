{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/dist/assets/$1"
        },
        {
            "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|json|woff|woff2|ttf|eot))",
            "dest": "/dist/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "regions": [
        "iad1"
    ]
}